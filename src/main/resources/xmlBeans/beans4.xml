<?xml version="1.0" encoding="UTF-8" ?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="collectionsBean" class="com.example.spring.demo.lesson2example3.CollectionInjection">
        <property name="list">
            <list>
                <value>One</value>
                <value>Two</value>
                <value>Three</value>
            </list>
        </property>
        <property name="map">
            <map>
                <entry key="1" value="One"/>
                <entry key="2" value="Two"/>
                <entry key="3" value="Three"/>
            </map>
        </property>
        <property name="set">
            <set>
                <value>One</value>
                <value>Two</value>
                <value>Three</value>
            </set>
        </property>
        <property name="props">
            <props>
                <prop key="one">1</prop>
                <prop key="two">2</prop>
                <prop key="three">3</prop>
            </props>
        </property>
    </bean>

</beans>